<div class="spots-results">
  <div class="spots-results__map-overlay spots-results__map-overlay--hide js-map-overlay">
    <%= render "shared/map" %>
  </div>
  <div class="cards-grid">
    <% @spots.each do |spot| %>
      <%= render partial: "spots/spot_card", locals: { spot: spot} %>
    <% end %>
  </div>
</div>
